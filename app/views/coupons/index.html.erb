<p style="color: green"><%= notice %></p>

<% content_for :title, "Coupons" %>

<h1>Coupons</h1>

<div id="coupons">
  <% @coupons.each do |coupon| %>
    <%= render coupon %>
    <p>
      <%= link_to "Show this coupon", coupon %>
    </p>
  <% end %>
</div>

<%= link_to "New coupon", new_coupon_path %>
